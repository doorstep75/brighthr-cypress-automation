{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["ESNext", "DOM"],
    "module": "NodeNext",
    "moduleResolution": "nodeNext",
    "esModuleInterop": true,
    "types": ["cypress", "node"],
    "resolveJsonModule": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["cypress/**/*.ts", "cypress/**/*.tsx", "cypress.config.ts"]
}
